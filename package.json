{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "react starter with exposed config",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel 7",
    "starter",
    "config"
  ],
  "author": "amazingrv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:prod",
    "build:dev": "npm run clean && webpack --config webpack.dev.js",
    "build:prod": "npm run clean && webpack --config webpack.prod.js",
    "start": "npm run clean && webpack-dev-server --config webpack.dev.js --hot --open"
  },
