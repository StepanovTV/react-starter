    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
